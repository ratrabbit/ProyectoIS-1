<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="mapeo.GaleriaPublicacion" table="galeria_publicacion" schema="public" optimistic-lock="version">
        <id name="idGaleria" type="int">
            <column name="id_galeria" />
            <generator class="assigned" />
        </id>
        <property name="urlImagen" type="string">
            <column name="url_imagen" not-null="true" />
        </property>
        <property name="varcharoImagen" type="string">
            <column name="varcharo_imagen" />
        </property>
    </class>
</hibernate-mapping>
